#include <stdio.h>
int main() {
    int nums[] = {1, 2, 3, 4, 5, 6};
    int n = sizeof(nums) / sizeof(nums[0]);
    for (int i = 0; i < n; i++) {
        if (nums[i] % 2 == 0) {
            printf("%d is even.\n", nums[i]);
        } else {
            printf("%d is odd.\n", nums[i]);
        }
    }
    return 0;
}
